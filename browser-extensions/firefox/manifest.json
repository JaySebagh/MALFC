{
    "manifest_version": 2,
    "name": "MALFC",
    "version": "1.0",

    "description": "MALFC browser extension for myanimelist.net",
    
    "icons": {
        "48": "icons/malfc-48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.myanimelist.net/character/*"],
            "js": ["malfc.js"]
        }
    ]
}